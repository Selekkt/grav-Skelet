<!-- CSS -->
{% block stylesheets %}
	{% do assets.addCss('https://rsms.me/inter/inter.css') %}
	{% do assets.addCss('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css') %} <!-- browse the icons: https://fontawesome.com/icons?d=listing&m=free -->
	{% do assets.addCss('theme://assets/css/skelet.css') %} <!-- include Skelet. -->
	{% do assets.addCss('theme://assets/css/app.css') %} <!-- Write all of your CSS styles here -->
{% endblock %}
{{ assets.css()|raw }}

<!-- JavaScript -->
{% block javascripts %}
	{% do assets.addJs('', {group: 'bottom'}) %}
	{% do assets.addJs('theme://assets/js/modules.js', {group:'bottom'}) %} <!-- Get all your modules in 1 file -->
	{% do assets.addJs('theme://assets/js/app.js', {group:'bottom'}) %} <!-- Write all of your JS here -->
{% endblock %}
{{ assets.js()|raw }}
